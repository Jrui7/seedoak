$(document).ready(function(){
  $("#close-modal").click();
  $(".pick-section").html('<%= j render 'seeds/pick-section' %>');
  $(".pick-container-xs").html('<%= j render 'seeds/pick-section-xs' %>');
  $(".my-picks").html('<%= j render 'shared/pending-picks' %> ');
  if ($(".complete-link").hasClass("shake")) {
    $(".complete-link").toggleClass("shake");
    $(".complete-link").toggleClass("shake-2");
  } else if ($(".complete-link").hasClass("shake-2")) {
    $(".complete-link").toggleClass("shake-2");
    $(".complete-link").toggleClass("shake");
  } else {
    $(".complete-link").toggleClass("shake");
  }
})
