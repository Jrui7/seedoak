<h1>Seeds#new</h1>
<div class="container">
  <div class="row">
     <div class="col-xs-12 col-sm-6 col-sm-offset-3 form">
       <div class="text-center">
        <h1>Ajouter un seed<h1>
        </div>
       <%= simple_form_for @seed do |b| %>
         <%= b.input :title %>
         <%= b.input :description %>
         <%= b.input :category_id, collection: @categories, prompt: "Select category"%>
         <%= b.input :photos, as: :attachinary %>
         <%= b.submit class: "text-center btn btn-primary" %>
       <% end %>
     </div>
 </div>
</div>

